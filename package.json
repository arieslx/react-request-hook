    "@types/react-dom": "^17.0.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.1.3"
  }
