{
  "name": "@react-cmpt/react-request-hook",
  "version": "2.1.1",
  "description": "Managed request calls made easy by React Hooks",
  "contributors": [
    "Matheus Schettino <schettino2@gmail.com>",
    "Dave Wang <wangcch.cc@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-cmpt/react-request-hook.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rm -rf ./lib/* ./esm/* && yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m ESNext --outDir ./esm",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "prettier": "prettier --write '**/*' --config ./.prettierrc",
    "prepare": "yarn build"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "peerDependencies": {
    "axios": ">=0.15",
    "react": ">=16.8.2",
    "react-dom": ">=16.8.2"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "prettier": "^2.2.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "2.1.5",
    "react-testing-library": "^6.0.0",
    "typescript": "^3.2.2"
  }
